<p id="notice"><%= notice %></p>

<h1>Listing Over Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @over_profiles.each do |over_profile| %>
      <tr>
        <td><%= link_to 'Show', over_profile %></td>
        <td><%= link_to 'Edit', edit_over_profile_path(over_profile) %></td>
        <td><%= link_to 'Destroy', over_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Over profile', new_over_profile_path %>
