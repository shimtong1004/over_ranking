<p id="notice"><%= notice %></p>

<h1>Listing Over Platforms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @over_platforms.each do |over_platform| %>
      <tr>
        <td><%= link_to 'Show', over_platform %></td>
        <td><%= link_to 'Edit', edit_over_platform_path(over_platform) %></td>
        <td><%= link_to 'Destroy', over_platform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Over platform', new_over_platform_path %>
